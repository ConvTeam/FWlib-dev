//#define FILE_LOG_SILENCE\par
#include "{\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAA" }{}}{\fldrslt {\cs37\ul\cf2 protocol/DNS/dns.h}}}
"\par
\par
\par
#define INITRTT     2000L   /* Initial smoothed response time */\par
#define MAXCNAME    10  /* Maximum amount of cname recursion */\par
\par
#define TYPE_A      1   /* Host address */\par
#define TYPE_NS     2   /* Name server */\par
#define TYPE_MD     3   /* Mail destination (obsolete) */\par
#define TYPE_MF     4   /* Mail forwarder (obsolete) */\par
#define TYPE_CNAME  5   /* Canonical name */\par
#define TYPE_SOA    6   /* Start of Authority */\par
#define TYPE_MB     7   /* Mailbox name (experimental) */\par
#define TYPE_MG     8   /* Mail group member (experimental) */\par
#define TYPE_MR     9   /* Mail rename name (experimental) */\par
#define TYPE_NULL   10  /* Null (experimental) */\par
#define TYPE_WKS    11  /* Well-known sockets */\par
#define TYPE_PTR    12  /* Pointer record */\par
#define TYPE_HINFO  13  /* Host information */\par
#define TYPE_MINFO  14  /* Mailbox information (experimental)*/\par
#define TYPE_MX     15  /* Mail exchanger */\par
#define TYPE_TXT    16  /* Text strings */\par
#define TYPE_ANY    255 /* Matches any type */\par
\par
#define CLASS_IN    1   /* The ARPA Internet */\par
\par
/* Round trip timing parameters */\par
#define AGAIN   8       /* Average RTT gain = 1/8 */\par
#define LAGAIN  3       /* Log2(AGAIN) */\par
#define DGAIN   4       /* Mean deviation gain = 1/4 */\par
#define LDGAIN  2       /* log2(DGAIN) */\par
\par
#define IPPORT_DOMAIN   53\par
#define MAX_DNS_BUF_SIZE    512     /* maximum size of DNS buffer. */\par
\par
/* Header for all domain messages */\par
struct dhdr\par
\{\par
    uint16 id;      /* Identification */\par
    uint8   qr;     /* Query/Response */\par
#define QUERY       0\par
#define RESPONSE    1\par
    uint8   opcode;\par
#define IQUERY      1\par
    uint8   aa;     /* Authoratative answer */\par
    uint8   tc;     /* Truncation */\par
    uint8   rd;     /* Recursion desired */\par
    uint8   ra;     /* Recursion available */\par
    uint8   rcode;      /* Response code */\par
#define NO_ERROR    0\par
#define FORMAT_ERROR    1\par
#define SERVER_FAIL 2\par
#define NAME_ERROR  3\par
#define NOT_IMPL    4\par
#define REFUSED     5\par
    uint16 qdcount; /* Question count */\par
    uint16 ancount; /* Answer count */\par
    uint16 nscount; /* Authority (name server) count */\par
    uint16 arcount; /* Additional record count */\par
\};\par
\par
static uint8 * put16(uint8 *s, uint16 i)\par
\{\par
    *s++ = i >> 8;\par
    *s++ = i;\par
\par
    return s;\par
\}\par
\par
static uint16 get16(uint8 *s)\par
\{\par
    uint16 i;\par
\par
    i = *s++ << 8;\par
    i = i + *s;\par
\par
    return i;\par
\}\par
\par
static int16 dns_makequery(uint16 op, int8 *name, uint8 *buf, uint16 len)\par
\{\par
    uint8 *cp;\par
    int8 *cp1;\par
    int8 sname[MAX_DNS_BUF_SIZE];\par
    int8 *dname;\par
    uint16 p;\par
    uint16 dlen;\par
    static uint16 MSG_ID = 0x1122;\par
\par
    cp = buf;\par
\par
    MSG_ID++;\par
    cp = put16(cp, MSG_ID);\par
    p = (op << 11) | 0x0100;            /* Recursion desired */\par
    cp = put16(cp, p);\par
    cp = put16(cp, 1);\par
    cp = put16(cp, 0);\par
    cp = put16(cp, 0);\par
    cp = put16(cp, 0);\par
\par
    strcpy((char*)sname, (char*)name);\par
    dname = sname;\par
    dlen = strlen((char*)dname);\par
    for (;;)\par
    \{\par
        /* Look for next dot */\par
        cp1 = (int8*)strchr((char*)dname, '.');\par
\par
        if (cp1 != NULL) len = cp1 - dname; /* More to come */\par
        else len = dlen;            /* Last component */\par
\par
        *cp++ = len;                /* Write length of component */\par
        if (len == 0) break;\par
\par
        /* Copy component up to (but not including) dot */\par
        strncpy((char*)cp, (char*)dname, len);\par
        cp += len;\par
        if (cp1 == NULL)\par
        \{\par
            *cp++ = 0;          /* Last one; write null and finish */\par
            break;\par
        \}\par
        dname += len+1;\par
        dlen -= len+1;\par
    \}\par
\par
    cp = put16(cp, 0x0001);             /* type */\par
    cp = put16(cp, 0x0001);             /* class */\par
\par
    return ((int16)(cp - buf));\par
\}\par
\par
static int32 parse_name(uint8 *msg, uint8 *compressed, int8 *buf, int16 len)\par
\{\par
    uint16 slen;        /* Length of current segment */\par
    uint8 * cp;\par
    int32 clen = 0;     /* Total length of compressed name */\par
    int32 indirect = 0; /* Set if indirection encountered */\par
    int32 nseg = 0;     /* Total number of segments in name */\par
\par
    cp = compressed;\par
\par
    for (;;)\par
    \{\par
        slen = *cp++;   /* Length of this segment */\par
\par
        if (!indirect) clen++;\par
\par
        if ((slen & 0xc0) == 0xc0)\par
        \{\par
            if (!indirect)\par
                clen++;\par
            indirect = 1;\par
            /* Follow indirection */\par
            cp = &msg[((slen & 0x3f)<<8) + *cp];\par
            slen = *cp++;\par
        \}\par
\par
        if (slen == 0)  /* zero length == all done */\par
            break;\par
\par
        len -= slen + 1;\par
\par
        if (len < 0) return -1;\par
\par
        if (!indirect) clen += slen;\par
\par
        while (slen-- != 0) *buf++ = (int8)*cp++;\par
        *buf++ = '.';\par
        nseg++;\par
    \}\par
\par
    if (nseg == 0)\par
    \{\par
        /* Root name; represent as single dot */\par
        *buf++ = '.';\par
        len--;\par
    \}\par
\par
    *buf++ = '\\0';\par
    len--;\par
\par
    return clen;    /* Length of compressed message */\par
\}\par
\par
static uint8 * dns_question(uint8 *msg, uint8 *cp)\par
\{\par
    int32 len;\par
    int8 name[MAX_DNS_BUF_SIZE];\par
\par
    len = parse_name(msg, cp, name, MAX_DNS_BUF_SIZE);\par
\par
    {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAB" }{}}{\fldrslt {\cs37\ul\cf2 DBGA}}}
("dns_question : %s", name);\par
\par
    if (len == -1) return 0;\par
\par
    cp += len;\par
    cp += 2;        /* type */\par
    cp += 2;        /* class */\par
\par
    return cp;\par
\}\par
\par
static uint8 * dns_answer(uint8 *msg, uint8 *cp, uint8 *pSip)\par
\{\par
    int32 len, type;\par
    int8 name[MAX_DNS_BUF_SIZE];\par
\par
    len = parse_name(msg, cp, name, MAX_DNS_BUF_SIZE);\par
\par
    if (len == -1) return 0;\par
\par
    cp += len;\par
    type = get16(cp);\par
    cp += 2;        /* type */\par
    cp += 2;        /* class */\par
    cp += 4;        /* ttl */\par
    cp += 2;        /* len */\par
\par
    {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAB" }{}}{\fldrslt {\cs37\ul\cf2 DBGA}}}
("dns_answer : %s : %u : %.4x", name, len, type);\par
\par
\par
    switch (type)\par
    \{\par
    case TYPE_A:\par
        /* Just read the address directly into the structure */\par
        //Server_IP_Addr[0] = *cp++;\par
        //Server_IP_Addr[1] = *cp++;\par
        //Server_IP_Addr[2] = *cp++;\par
        //Server_IP_Addr[3] = *cp++;\par
        //Config_Msg.Sip[0] = *cp++;\par
        //Config_Msg.Sip[1] = *cp++;\par
        //Config_Msg.Sip[2] = *cp++;\par
        //Config_Msg.Sip[3] = *cp++;\par
        pSip[0] = *cp++;\par
        pSip[1] = *cp++;\par
        pSip[2] = *cp++;\par
        pSip[3] = *cp++;\par
        break;\par
    case TYPE_CNAME:\par
    case TYPE_MB:\par
    case TYPE_MG:\par
    case TYPE_MR:\par
    case TYPE_NS:\par
    case TYPE_PTR:\par
        /* These types all consist of a single domain name */\par
        /* convert it to ascii format */\par
        len = parse_name(msg, cp, name, MAX_DNS_BUF_SIZE);\par
        if (len == -1) return 0;\par
\par
        cp += len;\par
        break;\par
    case TYPE_HINFO:\par
        len = *cp++;\par
        cp += len;\par
\par
        len = *cp++;\par
        cp += len;\par
        break;\par
    case TYPE_MX:\par
        cp += 2;\par
        /* Get domain name of exchanger */\par
        len = parse_name(msg, cp, name, MAX_DNS_BUF_SIZE);\par
        if (len == -1) return 0;\par
\par
        cp += len;\par
        break;\par
    case TYPE_SOA:\par
        /* Get domain name of name server */\par
        len = parse_name(msg, cp, name, MAX_DNS_BUF_SIZE);\par
        if (len == -1) return 0;\par
\par
        cp += len;\par
\par
        /* Get domain name of responsible person */\par
        len = parse_name(msg, cp, name, MAX_DNS_BUF_SIZE);\par
        if (len == -1) return 0;\par
\par
        cp += len;\par
\par
        cp += 4;\par
        cp += 4;\par
        cp += 4;\par
        cp += 4;\par
        cp += 4;\par
        break;\par
    case TYPE_TXT:\par
        /* Just stash */\par
        break;\par
    default:\par
        /* Ignore */\par
        break;\par
    \}\par
\par
    return cp;\par
\}\par
\par
static int8 parseMSG(struct dhdr *pdhdr, uint8 *pbuf, uint8 *pSip)\par
\{\par
    uint16 tmp;\par
    uint16 i;\par
    uint8 * msg;\par
    uint8 * cp;\par
\par
    msg = pbuf;\par
    memset(pdhdr, 0, sizeof(pdhdr));\par
\par
    pdhdr->id = get16(&msg[0]);\par
    tmp = get16(&msg[2]);\par
    if (tmp & 0x8000) pdhdr->qr = 1;\par
\par
    pdhdr->opcode = (tmp >> 11) & 0xf;\par
\par
    if (tmp & 0x0400) pdhdr->aa = 1;\par
    if (tmp & 0x0200) pdhdr->tc = 1;\par
    if (tmp & 0x0100) pdhdr->rd = 1;\par
    if (tmp & 0x0080) pdhdr->ra = 1;\par
\par
    pdhdr->rcode = tmp & 0xf;\par
    pdhdr->qdcount = get16(&msg[4]);\par
    pdhdr->ancount = get16(&msg[6]);\par
    pdhdr->nscount = get16(&msg[8]);\par
    pdhdr->arcount = get16(&msg[10]);\par
\par
    {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAB" }{}}{\fldrslt {\cs37\ul\cf2 DBGA}}}
("dhdr->qdcount : %x", (uint16)pdhdr->qdcount);\par
    {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAB" }{}}{\fldrslt {\cs37\ul\cf2 DBGA}}}
("dhdr->ancount : %x", (uint16)pdhdr->ancount);\par
    {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAB" }{}}{\fldrslt {\cs37\ul\cf2 DBGA}}}
("dhdr->nscount : %x", (uint16)pdhdr->nscount);\par
    {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAB" }{}}{\fldrslt {\cs37\ul\cf2 DBGA}}}
("dhdr->arcount : %x", (uint16)pdhdr->arcount);\par
\par
    /* Now parse the variable length sections */\par
    cp = &msg[12];\par
\par
    /* Question section */\par
    for (i = 0; i < pdhdr->qdcount; i++)\par
    \{\par
        cp = dns_question(msg, cp);\par
    \}\par
\par
    /* Answer section */\par
    for (i = 0; i < pdhdr->ancount; i++)\par
    \{\par
        cp = dns_answer(msg, cp, pSip);\par
    \}\par
\par
    /* Name server (authority) section */\par
    for (i = 0; i < pdhdr->nscount; i++)\par
    \{\par
        ;\par
    \}\par
\par
    /* Additional section */\par
    for (i = 0; i < pdhdr->arcount; i++)\par
    \{\par
        ;\par
    \}\par
\par
    if(pdhdr->rcode == 0) return {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAC" }{}}{\fldrslt {\cs37\ul\cf2 RET_OK}}}
;\par
    else return {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAD" }{}}{\fldrslt {\cs37\ul\cf2 RET_NOK}}}
;\par
\}\par
\par
int8 {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAE" }{}}{\fldrslt {\cs37\ul\cf2 dns_query}}}
(uint8 sock, uint8 *domain, uint8 *ip)\par
\{\par
    struct dhdr dhp;\par
    {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAF" }{}}{\fldrslt {\cs37\ul\cf2 wiz_NetInfo}}}
 netinfo;\par
    uint8 ip_tmp[4], i = 0;\par
    uint16 port;\par
    int32 len, cnt;\par
    uint32 tick;\par
    static uint8 dns_buf[MAX_DNS_BUF_SIZE];\par
\par
    tick = {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAG" }{}}{\fldrslt {\cs37\ul\cf2 wizpf_get_systick}}}
();\par
    srand(tick);\par
    while({\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAH" }{}}{\fldrslt {\cs37\ul\cf2 UDPOpen}}}
(sock, rand() % 5535 + 60000) != {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAC" }{}}{\fldrslt {\cs37\ul\cf2 RET_OK}}}
) \{\par
        if(i++ > 3) \{\par
            {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAB" }{}}{\fldrslt {\cs37\ul\cf2 DBGA}}}
("UDPOpen fail (%d)times", i);\par
            return {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAD" }{}}{\fldrslt {\cs37\ul\cf2 RET_NOK}}}
;\par
        \}\par
    \}\par
\par
    i = 0;\par
    {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAI" }{}}{\fldrslt {\cs37\ul\cf2 GetNetInfo}}}
(&netinfo);\par
    len = dns_makequery(0, (int8 *)domain, dns_buf, MAX_DNS_BUF_SIZE);\par
    while((cnt = {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAJ" }{}}{\fldrslt {\cs37\ul\cf2 UDPSend}}}
(sock, (int8*)dns_buf, len, netinfo.dns, IPPORT_DOMAIN)) < 0) \{\par
        if(i++ > 3) \{\par
            {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAB" }{}}{\fldrslt {\cs37\ul\cf2 DBGA}}}
("UDPSend fail (%d)times", i);\par
            return {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAD" }{}}{\fldrslt {\cs37\ul\cf2 RET_NOK}}}
;\par
        \}\par
    \}\par
    if (cnt == 0) return {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAD" }{}}{\fldrslt {\cs37\ul\cf2 RET_NOK}}}
; // dns fail\par
\par
    cnt = 0;\par
    while(1) \{\par
        if((len = {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAK" }{}}{\fldrslt {\cs37\ul\cf2 UDPRecv}}}
(sock, (int8*)dns_buf, MAX_DNS_BUF_SIZE, ip_tmp, &port)) > 0) \{\par
            {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAL" }{}}{\fldrslt {\cs37\ul\cf2 UDPClose}}}
(sock);\par
            break;\par
        \}\par
        {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAM" }{}}{\fldrslt {\cs37\ul\cf2 Delay_ms}}}
(10);\par
        if(cnt++ == 100) \{\par
            {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAL" }{}}{\fldrslt {\cs37\ul\cf2 UDPClose}}}
(sock);\par
            return {\field {\*\fldinst { HYPERLINK  \\l "AAAAAAAAAD" }{}}{\fldrslt {\cs37\ul\cf2 RET_NOK}}}
;\par
        \}\par
    \}\par
\par
    return parseMSG(&dhp, dns_buf, ip); /* Convert to local format */\par
\}\par
