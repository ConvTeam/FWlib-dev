\par \hich\af2\dbch\af31505\loch\f2 static int32 parse_name(uint8 *msg, uint8 *compressed, int8 *buf, int16 len)
\par \{
\par \hich\af2\dbch\af31505\loch\f2     uint16 slen;        /* Length of current segment */
\par \hich\af2\dbch\af31505\loch\f2     uint8 * cp;
\par \hich\af2\dbch\af31505\loch\f2     int32 clen = 0;     /* Total length of compressed name */
\par \hich\af2\dbch\af31505\loch\f2     int32 indirect = 0; /* Set if indirecti\hich\af2\dbch\af31505\loch\f2 on encountered */
\par \hich\af2\dbch\af31505\loch\f2     int32 nseg = 0;     /* Total number of segments in name */
\par 
\par \hich\af2\dbch\af31505\loch\f2     cp = compressed;
\par 
\par \hich\af2\dbch\af31505\loch\f2     for (;;)
\par \hich\af2\dbch\af31505\loch\f2     \{
\par \hich\af2\dbch\af31505\loch\f2         slen = *cp++;   /* Length of this segment */
\par 
\par \hich\af2\dbch\af31505\loch\f2         if (!indirect) clen++;
\par 
\par \hich\af2\dbch\af31505\loch\f2         if ((slen & 0xc0) == 0xc0)
\par \hich\af2\dbch\af31505\loch\f2         \{
\par \hich\af2\dbch\af31505\loch\f2   \hich\af2\dbch\af31505\loch\f2           if (!indirect)
\par \hich\af2\dbch\af31505\loch\f2                 clen++;
\par \hich\af2\dbch\af31505\loch\f2             indirect = 1;
\par \hich\af2\dbch\af31505\loch\f2             /* Follow indirection */
\par \hich\af2\dbch\af31505\loch\f2             cp = &msg[((slen & 0x3f)<<8) + *cp];
\par \hich\af2\dbch\af31505\loch\f2             slen = *cp++;
\par \hich\af2\dbch\af31505\loch\f2         \}
\par 
\par \hich\af2\dbch\af31505\loch\f2         if (slen == 0)  /* zero length == all done */
\par \hich\af2\dbch\af31505\loch\f2             break;
\par 
\par \hich\af2\dbch\af31505\loch\f2         len -= slen + 1;
\par 
\par \hich\af2\dbch\af31505\loch\f2         if (len < 0) return -1;
\par 
\par \hich\af2\dbch\af31505\loch\f2         if (!indirect) clen += slen;
\par 
\par \hich\af2\dbch\af31505\loch\f2         while (slen-- != 0) *buf++ = (int8)*cp++;
\par \hich\af2\dbch\af31505\loch\f2         *buf++ = '.';
\par \hich\af2\dbch\af31505\loch\f2         nseg++;
\par \hich\af2\dbch\af31505\loch\f2     \}
\par 
\par \hich\af2\dbch\af31505\loch\f2     if (nseg == 0)
\par \hich\af2\dbch\af31505\loch\f2     \{
\par \hich\af2\dbch\af31505\loch\f2         /* Root nam\hich\af2\dbch\af31505\loch\f2 e; represent as single dot */
\par \hich\af2\dbch\af31505\loch\f2         *buf++ = '.';
\par \hich\af2\dbch\af31505\loch\f2         len--;
\par \hich\af2\dbch\af31505\loch\f2     \}
\par 
\par \hich\af2\dbch\af31505\loch\f2     *buf++ = '\\0';
\par \hich\af2\dbch\af31505\loch\f2     len--;
\par 
\par \hich\af2\dbch\af31505\loch\f2     return clen;    /* Length of compressed message */
\par \}
